{
  "name": "moto-g5p-toolkit",
  "productName": "Moto G5+ Toolkit",
  "version": "1.0.1",
  "description": "Moto G5+ Toolkit built on top of Electron",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug --enable-logging",
    "test": "mocha && standard",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/Electron API Demos-win32-ia32/Electron API Demos.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js"
  },
  "repository": "https://github.com/drtechie/moto-g5p-toolkit",
  "keywords": [
    "Electron",
    "Moto G5+",
    "Root"
  ],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "devtron": "^1.3.0",
    "electron": "^1.6.6",
    "electron-connect": "^0.6.1",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.7",
    "electron-winstaller": "^2.2.0",
    "mocha": "^3.1.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.70.0",
    "rimraf": "^2.5.2",
    "signcode": "^0.5.0",
    "spectron": "~3.6.0",
    "standard": "^8.2.0",
    "extract-zip": "=1.6.0"
  },
  "dependencies": {
    "async-foreach": "^0.1.3",
    "better-console": "*",
    "deferred": "*",
    "del": "*",
    "electron-json-storage": "^3.0.4",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "electron-usb": "0.0.11",
    "extend": "*",
    "glob": "^7.1.0",
    "gulp": "*",
    "gulp-autoprefixer": "*",
    "gulp-chmod": "*",
    "gulp-clean-css": "*",
    "gulp-clone": "*",
    "gulp-concat": "*",
    "gulp-concat-css": "*",
    "gulp-copy": "*",
    "gulp-dedupe": "*",
    "gulp-flatten": "*",
    "gulp-header": "*",
    "gulp-help": "*",
    "gulp-if": "*",
    "gulp-less": "*",
    "gulp-notify": "*",
    "gulp-plumber": "*",
    "gulp-print": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-rtlcss": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "gulp-watch": "*",
    "highlight.js": "^9.3.0",
    "jquery": "^3.1.1",
    "keypress": "^0.2.1",
    "lodash": "*",
    "map-stream": "*",
    "nanotimer": "^0.3.14",
    "nedb": "^1.8.0",
    "request": "^2.72.0",
    "require-dot-file": "*",
    "run-sequence": "*",
    "semantic-ui": "^2.2.10",
    "yamljs": "*"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
