{
  "name": "moto-g5p-toolkit",
  "productName": "Moto G5+ Toolkit",
  "version": "0.9.0",
  "description": "Moto G5+ Toolkit built on top of Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug --enable-logging",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --asar=true",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico --asar=true",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out --asar=true",
    "postinstall" : "./node_modules/.bin/electron-rebuild",
    "release": "node ./script/release.js"
  },
  "repository": "https://github.com/drtechie/moto-g5p-toolkit",
  "keywords": [
    "Electron",
    "Android",
    "Toolkit",
    "Root"
  ],
  "author": "drtechie",
  "license": "MIT",
  "devDependencies": {
    "better-console": "*",
    "del": "*",
    "devtron": "^1.3.0",
    "electron": "^1.6.6",
    "electron-connect": "^0.6.1",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.7",
    "electron-winstaller": "^2.2.0",
    "extend": "*",
    "extract-zip": "=1.6.0",
    "gulp": "*",
    "gulp-autoprefixer": "*",
    "gulp-chmod": "*",
    "gulp-clean-css": "*",
    "gulp-clone": "*",
    "gulp-concat": "*",
    "gulp-concat-css": "*",
    "gulp-copy": "*",
    "gulp-dedupe": "*",
    "gulp-flatten": "*",
    "gulp-header": "*",
    "gulp-help": "*",
    "gulp-if": "*",
    "gulp-less": "*",
    "gulp-notify": "*",
    "gulp-plumber": "*",
    "gulp-print": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-rtlcss": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "gulp-watch": "*",
    "mocha": "^3.1.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.70.0",
    "rimraf": "^2.5.2",
    "semantic-ui": "=2.2.10",
    "signcode": "^0.5.0",
    "spectron": "~3.6.0",
    "standard": "^8.2.0",
    "run-sequence": "*",
    "yamljs": "*"
  },
  "dependencies": {
    "async-foreach": "^0.1.3",
    "electron-json-storage": "^3.0.4",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "electron-usb": "0.0.11",
    "glob": "^7.1.0",
    "highlight.js": "^9.3.0",
    "jquery": "^3.1.1",
    "lodash": "*",
    "map-stream": "*",
    "require-dot-file": "*"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
